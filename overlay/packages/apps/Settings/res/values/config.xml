    <!-- Whether to show Smooth Display feature in Settings Options -->
    <bool name="config_show_smooth_display">false</bool>

    <!-- Whether to show min refresh rate in display settings -->
    <bool name="config_show_min_refresh_rate_switch">false</bool>
    <bool name="config_supports_dynamic_refresh_rate_controls">false</bool>

    <!-- Battery Health -->
    <bool name="config_supportBatteryHealth">true</bool>
    <string name="config_batCurCap">/sys/class/power_supply/battery/charge_counter</string>
    <string name="config_batDesCap">/sys/class/power_supply/battery/charge_full_design</string>
    <string name="config_batChargeCycle">/sys/class/power_supply/battery/cycle_count</string>

